                <java classname="net.cbtltd.server.script.HandlerLauncher">
			            <jvmarg value="-Dbp.environment=production"/>
                        <arg value="${pms}"/>
                        <arg value="${method}"/>
                        <arg value="${partnerids}"/>
                        <classpath>
                                <path refid="Razor.classpath"/>
                        </classpath>
                </java>
	</target>
    	
